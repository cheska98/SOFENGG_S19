<AnchorPane fx:id="completePopup" prefHeight="231.0" prefWidth="521.0" style="-fx-border-width: 2px; -fx-border-color: #2d4250;" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TransactionController">
   <children>
      <Label layoutX="180.0" layoutY="35.0" prefHeight="40.0" prefWidth="200.0" text="Total Amount Paid">
         <font>
            <Font name="Segoe UI" size="20.0" />
         </font>
      </Label>
      <TextField fx:id="paidTF" layoutX="145.0" layoutY="92.0" prefHeight="40.0" prefWidth="248.0" />
      <Button fx:id="btnOK" layoutX="81.0" layoutY="169.0" mnemonicParsing="false" onAction="#handleOK" prefHeight="40.0" prefWidth="78.0" style="-fx-background-color: #289b01;" text="OK" textFill="#f7fcf2" />
      <ComboBox fx:id="debtListCB" layoutX="186.0" layoutY="169.0" onAction="#handleDebt" prefHeight="40.0" prefWidth="141.0" promptText="Add to Debt List" />
      <Button fx:id="btnCancel" layoutX="349.0" layoutY="169.0" mnemonicParsing="false" onAction="#handleCancel" prefHeight="40.0" prefWidth="105.0" style="-fx-background-color: #893737;" text="Cancel" textFill="WHITE">
